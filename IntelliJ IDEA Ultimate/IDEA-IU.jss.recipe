<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Description</key>
  <string>Downloads the current release version of Intellij IDEA and imports into a JAMF Pro Server.</string>
  <key>Identifier</key>
  <string>com.github.txhaflaire.jss.IDEA-IU</string>
  <key>Input</key>
  <dict>
    <key>CATEGORY</key>
    <string>Software - Licensed</string>
    <key>NAME</key>
    <string>IntelliJ Idea Ultimate</string>
    <key>POLICY_CATEGORY</key>
    <string>Software - Licensed</string>
    <key>POLICY_TEMPLATE</key>
    <string>IntellijIDEAPolicyTemplate.xml</string>
    <key>SELF_SERVICE_DESCRIPTION</key>
    <string>Install the latest version of IntelliJ Idea Ultimate on your device.</string>
    <key>SELF_SERVICE_ICON</key>
    <string>IDEA-IU.png</string>
  </dict>
  <key>MinimumVersion</key>
  <string>0.4.0</string>
  <key>ParentRecipe</key>
  <string>com.github.mosen.download.IDEA-IU</string>
  <key>Process</key>
  <array>
    <dict>
      <key>Arguments</key>
      <dict>
        <key>category</key>
        <string>%CATEGORY%</string>
        <key>policy_category</key>
        <string>%POLICY_CATEGORY%</string>
        <key>policy_template</key>
        <string>%POLICY_TEMPLATE%</string>
        <key>prod_name</key>
        <string>%NAME%</string>
        <key>self_service_description</key>
        <string>%SELF_SERVICE_DESCRIPTION%</string>
        <key>self_service_icon</key>
        <string>%SELF_SERVICE_ICON%</string>
        <key>pkg_path</key>
        <string>%pathname%</string>
      </dict>
      <key>Processor</key>
      <string>JSSImporter</string>
    </dict>
  </array>
</dict>
</plist>